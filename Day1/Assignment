//first question

#include <bits/stdc++.h>
using namespace std;
void reverseArray(int arr[], int start, int end)
{
    while (start < end)
    {
        int temp = arr[start];
        arr[start] = arr[end];
        arr[end] = temp;
        start++;
        end--;
    }
}    
 
void printArray(int arr[], int size)
{
   for (int i = 0; i < size; i++)
   cout << arr[i] << " ";
   cout << endl;
}
 
int main()
{
    int arr[]= {4,5,6}; 
    int p = sizeof(arr) / sizeof(arr[0]);
    printArray(arr, p);
    reverseArray(arr, 0, p-1);
    cout << "Reversed array is q =" << endl;
    printArray(arr, p); 
    return 0;
}


